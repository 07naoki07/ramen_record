<h1>投稿編集</h1>
 <%= form_with model: @record,:url => {:action => :update} do |f| %>
  <h4>日付</h4>
    <%= f.date_field :created_at %>
  <h4>画像</h4>
    <%= f.file_field :image, accept: "image/*" %>
  <h4>ラーメン店名</h4>
    <%= f.text_field :shop_name %>
  <h4>ラーメン名</h4>
    <%= f.text_field :name %>
  <h4>説明</h4>
      <%= f.text_area :caption %>
    <%= f.submit '投稿' %>
 <% end %>